#declare the guess number
dci a
#declare the secret number
dci b
#input the secret number
rdi b
L1: prt “The secret number is larger”
L2: prt “The secret number is smaller”
#user guess a number
rdi a
#check if user guess the secret number
cmp a b
#if a<b, print “The secret number is larger”
jls L1
#if a>b, print “The secret number is smaller”
jmr L2
#if a=b, print ”You guess the secret number”
jeq L3
